#include <iostream>

using namespace std;

int main()
{
   int n,max,min;
   cin>>n;
   int count1=0;
   int count2=0;
   int A[n];
   for (int i=0;i<n;i++)
      {
       cin>>A[i];
      }
    max=A[0];
    for(int i=0;i<n;i++)
    {
        if(max<A[i])
        {
            max=A[i];
            count1++;
        }
    }
    min=A[0];
     for(int i=0;i<n;i++)
    {
        if(min>A[i])
        {
            min=A[i];
            count2++;
        }
    }
    cout<<count1<<"\t"<<count2;
    return 0;
}
   
